<%= render 'shared/navbarburger-notes'%>
<div class="container-note">
  <p class="conversation-name"><strong><%= @family.name %> / <%= @note.title %></strong></p>
  <div class="container">
    <div class="border p-2 members note-content">
      <%= @note.content %>
    </div>
  </div>
</div>
<div class="container">

<div class="border p-2 members">
        <h5>Collaborators:</h5>
        <% @users.each do |u| %>
        <%= u.username %>
        <br>
        <% end %>
        <br>
      </div>

</div>
